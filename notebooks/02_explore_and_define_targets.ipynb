# Cell 1: Setup
import sys
sys.path.append('..')

import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns
from scripts.download_manager_v2 import RobustDataManager

# Set style
sns.set_style('whitegrid')
plt.rcParams['figure.figsize'] = (12, 6)

print("‚úÖ Setup complete")

# Cell 2: Load the data
manager = RobustDataManager(
    use_aws=True,
    bucket_name='ad-rnaseq-prediction-data'
)

# Load from S3 or local
expr, meta = manager.load_from_s3('GSE63061')
print(f"üìä Loaded data: {expr.shape[0]} samples, {expr.shape[1]} genes")

# Cell 3: Explore the metadata
print("="*60)
print("üîç METADATA EXPLORATION")
print("="*60)

print("\nüìã Metadata columns:")
print(meta.columns.tolist())

print("\nüè• Diagnosis distribution:")
print(meta['diagnosis'].value_counts())
print(f"\nKey finding: We have {(meta['diagnosis']=='MCI_converter').sum()} MCI converters!")

print("\nüë• Demographics:")
print(f"Age: {meta['age'].mean():.1f} ¬± {meta['age'].std():.1f} years")
print(f"Sex distribution: {meta['sex'].value_counts().to_dict()}")
print(f"MMSE: {meta['MMSE'].mean():.1f} ¬± {meta['MMSE'].std():.1f}")

# Cell 4: Define your prediction targets
print("="*60)
print("üéØ DEFINING PREDICTION TARGETS")
print("="*60)

# PRIMARY TARGET 1: MCI to AD Conversion (Binary)
print("\n1Ô∏è‚É£ PRIMARY TARGET: MCI ‚Üí AD Conversion")
mci_samples = meta[meta['diagnosis'].isin(['MCI', 'MCI_converter'])]
print(f"   MCI stable: {(mci_samples['diagnosis']=='MCI').sum()}")
print(f"   MCI converters: {(mci_samples['diagnosis']=='MCI_converter').sum()}")
print(f"   Total MCI samples: {len(mci_samples)}")
print(f"   Conversion rate: {(mci_samples['diagnosis']=='MCI_converter').sum()/len(mci_samples)*100:.1f}%")

# Create binary labels
meta['will_convert'] = (meta['diagnosis'] == 'MCI_converter').astype(int)

# SECONDARY TARGET 2: Multi-class Classification
print("\n2Ô∏è‚É£ SECONDARY TARGET: Disease State Classification")
print("   Classes: Control, MCI, AD")
print("   Sample distribution:")
for diag in ['Control', 'MCI', 'AD']:
    samples = meta[meta['diagnosis'].str.contains(diag, na=False)]
    print(f"   - {diag}: {len(samples)} samples")

# SECONDARY TARGET 3: Cognitive Score Prediction
print("\n3Ô∏è‚É£ ADDITIONAL TARGET: MMSE Score Prediction")
print(f"   MMSE range: {meta['MMSE'].min():.0f} - {meta['MMSE'].max():.0f}")
print(f"   Mean by group:")
for diag in meta['diagnosis'].unique():
    mean_mmse = meta[meta['diagnosis']==diag]['MMSE'].mean()
    print(f"   - {diag}: {mean_mmse:.1f}")

# Cell 5: Visualize the targets
fig, axes = plt.subplots(2, 3, figsize=(15, 10))

# 1. Diagnosis distribution
ax = axes[0, 0]
meta['diagnosis'].value_counts().plot(kind='bar', ax=ax, color='skyblue')
ax.set_title('Sample Distribution by Diagnosis', fontweight='bold')
ax.set_xlabel('Diagnosis')
ax.set_ylabel('Count')
ax.tick_params(axis='x', rotation=45)

# 2. Age distribution by diagnosis
ax = axes[0, 1]
for diag in meta['diagnosis'].unique():
    subset = meta[meta['diagnosis']==diag]['age']
    ax.hist(subset, alpha=0.5, label=diag, bins=15)
ax.set_title('Age Distribution by Diagnosis', fontweight='bold')
ax.set_xlabel('Age')
ax.set_ylabel('Count')
ax.legend()

# 3. MMSE by diagnosis
ax = axes[0, 2]
diagnoses = ['Control', 'MCI', 'MCI_converter', 'AD']
mmse_data = [meta[meta['diagnosis']==d]['MMSE'].values for d in diagnoses]
bp = ax.boxplot(mmse_data, labels=diagnoses, patch_artist=True)
for patch, color in zip(bp['boxes'], ['green', 'yellow', 'orange', 'red']):
    patch.set_facecolor(color)
    patch.set_alpha(0.5)
ax.set_title('MMSE Scores by Diagnosis', fontweight='bold')
ax.set_ylabel('MMSE Score')
ax.tick_params(axis='x', rotation=45)

# 4. Conversion rate visualization
ax = axes[1, 0]
mci_data = meta[meta['diagnosis'].isin(['MCI', 'MCI_converter'])]
conversion_counts = pd.Series({
    'Stable MCI': (mci_data['diagnosis']=='MCI').sum(),
    'MCI‚ÜíAD': (mci_data['diagnosis']=='MCI_converter').sum()
})
colors = ['#2ecc71', '#e74c3c']
wedges, texts, autotexts = ax.pie(conversion_counts.values, 
                                   labels=conversion_counts.index, 
                                   autopct='%1.1f%%',
                                   colors=colors,
                                   startangle=90)
ax.set_title('MCI Conversion Rate', fontweight='bold')

# 5. Sex distribution
ax = axes[1, 1]
sex_by_diag = meta.groupby(['diagnosis', 'sex']).size().unstack()
sex_by_diag.plot(kind='bar', stacked=True, ax=ax, color=['lightblue', 'pink'])
ax.set_title('Sex Distribution by Diagnosis', fontweight='bold')
ax.set_xlabel('Diagnosis')
ax.set_ylabel('Count')
ax.tick_params(axis='x', rotation=45)
ax.legend(title='Sex')

# 6. Feature correlation heatmap
ax = axes[1, 2]
clinical_features = meta[['age', 'MMSE', 'will_convert']].copy()
clinical_features['sex_binary'] = (meta['sex'] == 'M').astype(int)
corr = clinical_features.corr()
sns.heatmap(corr, annot=True, fmt='.2f', cmap='coolwarm', center=0, ax=ax)
ax.set_title('Clinical Feature Correlations', fontweight='bold')

plt.tight_layout()
plt.show()

# Cell 6: Create analysis-ready datasets
print("="*60)
print("üì¶ CREATING ANALYSIS-READY DATASETS")
print("="*60)

# Dataset 1: MCI Conversion Prediction
mci_indices = meta['diagnosis'].isin(['MCI', 'MCI_converter'])
X_mci = expr[mci_indices]
y_mci = meta[mci_indices]['will_convert']
meta_mci = meta[mci_indices]

print(f"\n1Ô∏è‚É£ MCI Conversion Dataset:")
print(f"   X shape: {X_mci.shape}")
print(f"   y distribution: {y_mci.value_counts().to_dict()}")
print(f"   Class balance: {y_mci.mean()*100:.1f}% converters")

# Dataset 2: Multi-class (excluding converters for cleaner classes)
multiclass_indices = meta['diagnosis'].isin(['Control', 'MCI', 'AD'])
X_multi = expr[multiclass_indices]
y_multi = meta[multiclass_indices]['diagnosis']
meta_multi = meta[multiclass_indices]

print(f"\n2Ô∏è‚É£ Multi-class Dataset:")
print(f"   X shape: {X_multi.shape}")
print(f"   Classes: {y_multi.value_counts().to_dict()}")

# Dataset 3: Full dataset for MMSE regression
X_full = expr
y_mmse = meta['MMSE']
meta_full = meta

print(f"\n3Ô∏è‚É£ MMSE Regression Dataset:")
print(f"   X shape: {X_full.shape}")
print(f"   MMSE range: {y_mmse.min():.0f} - {y_mmse.max():.0f}")

# Save the prepared datasets
import os
os.makedirs('data/processed', exist_ok=True)

# Save locally for quick access
X_mci.to_csv('data/processed/X_mci_conversion.csv.gz', compression='gzip')
y_mci.to_csv('data/processed/y_mci_conversion.csv')
meta_mci.to_csv('data/processed/meta_mci_conversion.csv')

print("\n‚úÖ Datasets saved to data/processed/")
print("Ready for feature engineering and model building!")