# Cell 1: Load processed data
import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns

# Load original and processed
expr_processed = pd.read_csv('data/processed/expression_processed.csv.gz', compression='gzip', index_col=0)
meta_processed = pd.read_csv('data/processed/metadata_processed.csv', index_col=0)
selected_genes = pd.read_csv('data/processed/selected_genes.csv')['gene'].values

print("ðŸ“Š Data Processing Summary")
print("="*40)
print(f"Samples: {expr_processed.shape[0]}")
print(f"Genes after filtering: {expr_processed.shape[1]}")
print(f"Selected features: {len(selected_genes)}")

# Cell 2: Visualize data distribution
fig, axes = plt.subplots(1, 2, figsize=(12, 5))

# Before/after distribution
ax = axes[0]
expr_subset = expr_processed.iloc[:, :100]  # First 100 genes
ax.hist(expr_subset.values.flatten(), bins=50, alpha=0.7)
ax.set_title('Expression Distribution (Processed)')
ax.set_xlabel('Normalized Expression')
ax.set_ylabel('Frequency')

# PCA visualization
from sklearn.decomposition import PCA
ax = axes[1]
pca = PCA(n_components=2)
pca_coords = pca.fit_transform(expr_processed)

# Color by diagnosis
diagnosis_colors = {'Control': 'green', 'MCI': 'yellow', 
                   'MCI_converter': 'orange', 'AD': 'red'}
colors = [diagnosis_colors[d] for d in meta_processed['diagnosis']]

scatter = ax.scatter(pca_coords[:, 0], pca_coords[:, 1], 
                    c=colors, alpha=0.6, s=50)
ax.set_xlabel(f'PC1 ({pca.explained_variance_ratio_[0]:.1%})')
ax.set_ylabel(f'PC2 ({pca.explained_variance_ratio_[1]:.1%})')
ax.set_title('PCA of Processed Data')

# Add legend
for diag, color in diagnosis_colors.items():
    ax.scatter([], [], c=color, label=diag)
ax.legend()

plt.tight_layout()
plt.show()

# Cell 3: Show feature selection results
print("\nðŸŽ¯ Top 10 Selected Genes:")
for i, gene in enumerate(selected_genes[:10], 1):
    print(f"{i:2d}. {gene}")